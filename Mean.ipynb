    "# Q.19.Ans.\n",
    "\n",
    "def calculate_mean(n):\n",
    "    total = sum(n)\n",
    "    count = len(n)\n",
    "    \n",
    "    mean = total/count\n",
    "    return mean\n",
    "\n",
    "n= [10, 15, 20, 25, 30]\n",
    "\n",
    "print(calculate_mean(n))\n",
